					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200 OK\", function () {\r",
							"    if (pm.response.code === 200) {\r",
							"        console.log(\"Status code is 200 OK\");\r",
							"    } else {\r",
							"        console.error(\"Status code is not 200 OK. Actual status code: \" + pm.response.code);\r",
							"    }\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response body is a JSON object\", function () {\r",
							"    pm.response.to.be.json;\r",
							"});\r",
							"\r",
							"pm.expect(pm.response.responseTime).to.be.below(3000); \r",
							"\r",
							"pm.test(\"All books were found\", function () {\r",
							"    const responseBody = pm.response.json();\r",
							"    if (pm.response.code === 200 && Array.isArray(responseBody) && responseBody.length > 0) {\r",
							"        console.log(\"All books were found successfully.\");\r",
							"    } else {\r",
							"        console.error(\"No books were found or response is not valid.\");\r",
							"    }\r",
							"    pm.expect(responseBody).to.be.an('array').that.is.not.empty;\r",
							"});"
